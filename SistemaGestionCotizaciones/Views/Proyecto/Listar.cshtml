            });
            /*----------------*/
            $('#hddProrrateo').val(xmlProrrata);

            $.ajax({
                type: "POST",
                url: '@Url.Action("vpIngresar")',
                data: $("#form0").serialize(),
                async: false,
                success: function (result) {
                    /* if (result.length > 0) {
                         alert(result);
                     }
                     else {*/

        $('#loadingDiv').hide();
        $(document).ajaxStart(function () {
            $('#loadingDiv').show();  // show Loading Div
        }).ajaxStop(function () {
            $('#loadingDiv').hide(); // hide loading div
        });

        $('#tb_pytc').load('@Url.Action("vpGrillaListar", "Proyecto")', {

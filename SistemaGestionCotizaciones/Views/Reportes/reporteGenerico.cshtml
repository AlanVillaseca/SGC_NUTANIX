            $('#combo_pais').append('<option id="' + i.Idpais + '">' + i.Nombre + '</option>');
        });

        _.each(estados, function(i){
            $('#lis_estados').append('<li class="list-group-item"><input type="checkbox" id="'+ i.idestado +'"  >'+i.descripcion+'</input></li>');


    $(document).ready(function(){
        @{JavaScriptSerializer jss = new JavaScriptSerializer();}

        var paises = @Html.Raw(jss.Serialize(Model.dtPaisJson));
        var negocios = @Html.Raw(jss.Serialize(Model.dtNegocioPaisJson));

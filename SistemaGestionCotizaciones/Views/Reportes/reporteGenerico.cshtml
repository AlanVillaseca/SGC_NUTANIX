            $('#combo_elemento2').append('<option id="' + i.idCatalogoElemento + '">' + i.Nombreatributo + '</option>');
            $('#combo_elemento3').append('<option id="' + i.idCatalogoElemento + '">' + i.Nombreatributo + '</option>');

        /*
        _.each(serneg, function(i){
            $('#combo_serneg').append('<option id="' + i.Idserviciodenegocio + '">' + i.Nombre + '</option>');
        });*/
        //Leenado negocios cuando se selecione un pais
        $( "#combo_pais" ).change(function() {

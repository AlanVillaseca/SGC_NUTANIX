                    }

                });


                if (existe == 0) {

                    var url = '@Html.Raw(Url.Action("vpAgregarServicioNegocio", "Mantenedor", new { id = "reemplazo", idnegocio = "reemplazo2", nombre = "reemplazo3", descripcion = "reemplazo4" }))'


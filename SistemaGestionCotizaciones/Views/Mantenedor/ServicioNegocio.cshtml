            $('.btnEliminar').click(function () {


                var temp = $(this).attr('id');

                var callback = function () {

                    var url = '@Url.Action("eliminarServicioNegocio", "Mantenedor")'

                    $.ajax({
                        type: 'POST',
                        url: url,
                        data: {id: temp},
                        async: false
                    });

                    location.reload();


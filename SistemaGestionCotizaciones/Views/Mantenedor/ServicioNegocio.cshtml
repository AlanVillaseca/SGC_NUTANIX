        //////////////////////////////////////////////////////////////

        $('#guardar_serv_neg').click(function() {

            var idpais = $("#serv_neg_sel option:selected" ).attr('idpais');
            var idnegocio = $("#serv_neg_sel option:selected" ).attr('idnegocio');

